{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "linebreak-style": ["error", "windows"],
        "eol-last": 0,
        "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }],
        "require-jsdoc": "off",
        "quotes": "off",
        "operator-linebreak": "off",
        "no-unused-vars": "off"
    },
    "extends": ["eslint:recommended", "google"]
}